<?xml version="1.0" encoding="utf-8" ?>
<sourceamazing xmlns="https://codeblessing.org/sourceamazing/sourceamazing-xml-schema"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="https://codeblessing.org/sourceamazing/sourceamazing-xml-schema ./schema/sourceamazing-xml-schema.xsd">
    <definitions>
        <entity name="Person" alternativeName="PERSON">
            <entityAttribute attributeName="firstname" attributeType="TEXT"/>
            <entityAttribute attributeName="lastname" attributeType="TEXT"/>
            <entityAttribute attributeName="nickname" attributeType="TEXT"/>
        </entity>
        <entity name="Address">
            <entityAttribute attributeName="street" attributeType="TEXT"/>
            <entityAttribute attributeName="zip" attributeType="NUMBER"/>
        </entity>
    </definitions>
</sourceamazing>
